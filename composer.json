{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org"
    }
  ],
  "keywords": [
    "TYPO3 CMS",
    "Static Info Tables"
  ],
  "name": "mselbach/static-info-tables-ru",
  "description": "(ru) language pack for the Static Info Tables providing localized names for countries, currencies and so on.",
  "version": "6.3.1",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Manuel Selbach",
      "role": "Developer"
    }
  ],
  "replace": {
    "static_info_tables_ru": "self.version",
    "typo3-ter/static-info-tables-ru": "self.version"
  },
  "require": {
    "typo3/cms": ">=6.2,<8.0",
    "sjbr/static-info-tables": "6.3.*"
  },

  "require-dev": {
    "namelesscoder/typo3-repository-client": "1.2.*",
    "squizlabs/php_codesniffer": "^2.6",
    "phpunit/phpunit": "~4.8.0"
  },
  "autoload": {
    "psr-4": {
      "Mselbach\\StaticInfoTablesRu\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mselbach\\StaticInfoTablesRu\\Tests\\": "Tests",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "Package": {
        "partOfMinimalUsableSystem": true
      }
    }
  }
}
